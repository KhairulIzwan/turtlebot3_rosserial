<launch>
  <node pkg="rosserial_python"
        type="serial_node.py"
        name="publish_rand_no"
        output="screen">

        <param name="port" value="/dev/ttyUSB0"/>
        <param name="baud" value="115200"/>
  </node>

  <node pkg="rosserial_python"
        type="serial_node.py"
        name="subscribe_rand_no"
        output="screen">

        <param name="port" value="/dev/ttyUSB1"/>
        <param name="baud" value="115200"/>
  </node>

  <!-- <node
    name="random_number"
    pkg="turtlebot3_rosserial"
    type="displayCal.py"
    output="screen" /> -->
</launch>
